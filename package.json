{
  "name": "suzuri-lite",
  "version": "1.0.0",
  "description": "Experimental project that imitate suzuri using public API",
  "author": "Joe-noh",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:app": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev:proxy": "node server/dev-proxy.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "prettier --write ./**/*.{js,scss,vue}",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/dotenv": "^1.1.1",
    "@nuxtjs/font-awesome": "^1.0.3",
    "@nuxtjs/pwa": "^2.0.8",
    "@nuxtjs/router": "^1.0.1",
    "body-parser": "^1.18.3",
    "express": "^4.15.3",
    "express-http-proxy": "^1.2.0",
    "http-proxy": "^1.17.0",
    "nuxt": "^1.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "cross-env": "^5.0.1",
    "node-sass": "^4.9.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "sass-loader": "^7.0.3"
  }
}
